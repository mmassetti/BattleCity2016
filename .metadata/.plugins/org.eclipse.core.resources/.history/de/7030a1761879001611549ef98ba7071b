	public Mapa(Gui gui) {
			
			miJugador = new Jugador(15, 10, 10);
			gui.add(miJugador.getGrafico());
			
			
	}
		
		
	
	
	//Metodos
	
	
	public void finJuego() {
		
	}
	
	
}
